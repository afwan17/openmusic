{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 2020
  },
  "rules": {
      "no-console": "off",
      "no-underscore-dangle": [
          "error",
          { 
            "allowAfterThis": true // Mengizinkan garis bawah setelah `this` (contoh: `this._notes`)
          }
        ]
  }
}
